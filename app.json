{
    "expo": {
        "name": "Glynet Demo",
        "slug": "Glynet",
        "description": "New shape of social media",
        "version": "1.4.0",
        "privacy": "unlisted",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "platforms": ["ios", "android"],
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
